{
  "name": "magespices/bitcan",
  "type": "magento2-module",
  "license": "OSL-3.0",
  "description": "Magento 2 Bitcan payment gateway",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "magento/framework": ">=102.0.0",
    "magento/module-sales": ">=102.0.0",
    "magento/module-payment": ">=102.0.0",
    "magento/module-checkout": ">=102.0.0"
  },
  "autoload": {
    "files": [
      "src/module-bitcan/registration.php"
    ],
    "psr-4": {
      "Magespices\\Bitcan\\": "src/module-bitcan"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}